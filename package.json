        "@tailwindcss/typography": "^0.5.19",
        "@types/node": "^24.10.1",
        "node-fetch": "^3.3.2",
        "pocketbase-typegen": "^1.3.2",
        "prettier": "^3.7.4",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "sharp": "^0.34.5",
        "svelte": "^5.45.5",
        "svelte-check": "^4.3.4",
        "svgo": "^4.0.0",
        "tailwindcss": "^4.1.17",
